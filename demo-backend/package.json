{
  "name": "express-es6-rest-api-auth",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API with JWT auth",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "NODE_ENV=test mocha src --recursive --timeout 10000 --compilers js:babel-register --require babel-polyfill"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0
    }
  },
  "repository": "damianmarek/express-es6-rest-api-auth",
  "author": "Damian Marek <mail@damianmarek.me>",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "esformatter": "^0.10.0",
    "esformatter-align": "^0.2.3",
    "esformatter-quotes": "^1.1.0",
    "express": "^4.13.3",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.9.4",
    "mongoose-data-seed": "^1.0.6",
    "morgan": "^1.8.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "resource-router-middleware": "^0.6.0",
    "sinon": "^6.0.0",
    "string-format": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2"
  }
}
